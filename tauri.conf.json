{
  "package": {
    "productName": "countdown-system-tray",
    "version": "0.0.0"
  },
  "build": {
    "devPath": "http://localhost:8080/",
    "distDir": "icons",
    "withGlobalTauri": false
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "dev.moritzreich.countdown-system-tray",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    }
  }
}
